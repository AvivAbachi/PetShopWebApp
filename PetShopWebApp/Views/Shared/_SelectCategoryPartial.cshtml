@model IEnumerable<PetShopWebApp.Models.Category>

<select id="selectCategory" name="id" class="form-select">
	<option value="" disabled selected hidden>Select pet's category</option>
	<option value="">All</option>
	@foreach (var category in Model)
	{
		<option value="@(category.CategoryId)">@category.Name</option>
	}
</select>